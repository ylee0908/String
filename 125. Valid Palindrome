#time, space : O(n)
def isPalindrome(self, s):
        s = ''.join(e for e in s if e.isalnum()).lower()
        return s==s[::-1]
        
 
      
 def is_palindrom_two(s):
	s = ''.join(i for i in s if i.isalnum()).lower()
	
	i = 0
	j = len(s) - 1
	while i < j:
		if s[i] != s[j]:
			return False
		i += 1
		j -= 1
	return True
